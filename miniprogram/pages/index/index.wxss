/* ===== 交互式日历样式 ===== */

/* 日历导航按钮 */
.calendar-nav-btn {
  padding: 8rpx;
  border-radius: 50%;
  background-color: #f8f9fa;
  transition: background-color 0.3s ease;
}

.calendar-nav-btn:active {
  background-color: #e9ecef;
}

/* 日历网格容器 */
.calendar-grid {
  margin-top: 20rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 5rpx;
}

/* 日历行 */
.calendar-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8rpx;
  width: 100%;
  box-sizing: border-box;
  height: 40rpx;
  padding: 0 5rpx;
}

/* 日历日期单元格 */
.calendar-day {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  transition: all 0.3s ease;
  position: relative;
  box-sizing: border-box;
  background-color: transparent;
  flex-shrink: 0;
  margin: 0 1rpx;
}

/* 当前月日期 */
.calendar-day.current-month {
  background-color: transparent;
}

.calendar-day.current-month:active {
  background-color: #f8f9fa;
}

/* 非当前月日期 */
.calendar-day.prev-month,
.calendar-day.next-month {
  color: #d0d5db;
}

/* 今日日期 */
.calendar-day.today {
  background-color: #b2976433;
  color: #b29764;
  font-weight: 700;
  border-radius: 50%;
}

/* 已打卡日期 */
.calendar-day.checked {
  background-color: #b297641a;
  color: #b29764;
  font-weight: 700;
}

/* 日期文字 */
.day-text {
  font-size: 24rpx;
  font-family: Arimo;
  line-height: 15rpx;
  color: inherit;
}

/* 打卡图标 - 右上角 */
.check-icon {
  position: absolute;
  top: 2rpx;
  right: 2rpx;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #B29764;
  box-shadow: 0rpx 1rpx 2rpx rgba(0, 0, 0, 0.2);
}

/* 静坐打卡按钮容器 */
.meditation-btn-container {
  position: relative;
  z-index: 20;
  margin: 40rpx 28.24rpx 20rpx;
  padding: 10rpx 0;
  background-color: transparent;
  min-height: 80rpx;
}

/* WeUI规范静坐打卡按钮 */
.meditation-btn {
  background-color: #B29764;
  border-radius: 16rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  transition: all 0.3s ease;
  width: 100%;
  margin-bottom: 0;
}

.meditation-btn:active {
  transform: scale(0.98);
  box-shadow: 0rpx 2rpx 4rpx rgba(178, 151, 100, 0.3);
}

.meditation-btn-icon {
  width: 52rpx;
  height: 52rpx;
  margin-right: 24rpx;
}

.meditation-btn-text {
  font-size: 38rpx;
  font-family: Arimo;
  font-weight: 600;
  line-height: 1;
}

/* 图例图标样式 */
.check-indicator {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #B29764;
  display: block;
}

.today-indicator {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #F0EAE0;
  display: block;
}

/* 授权按钮样式 */
.auth-button {
  background-color: #b29764 !important;
  color: white !important;
  border-radius: 20rpx !important;
  font-size: 24rpx !important;
  padding: 8rpx 16rpx !important;
  border: none !important;
  line-height: 1.2 !important;
  height: auto !important;
  min-width: 100rpx !important;
}

.auth-button::after {
  border: none !important;
}

.auth-button:active {
  background-color: #9a8254 !important;
  transform: scale(0.95);
}

/* 点击登录文本按钮样式 */
.login-text {
  color: #fffefecc !important;
  font-size: 26rpx !important;
  font-family: Arimo !important;
  line-height: 23.45rpx !important;
  background-color: transparent !important;
  border: 1px solid rgba(255, 255, 255, 0.3) !important;
  border-radius: 20rpx !important;
  padding: 8rpx 16rpx !important;
  margin-left: 15rpx !important;
  text-decoration: none !important;
  cursor: pointer !important;
  transition: all 0.3s ease !important;
  display: inline-block !important;
  min-width: 120rpx !important;
  text-align: center !important;
  position: relative !important;
  z-index: 10 !important;
}

.login-text:active {
  background-color: rgba(255, 255, 255, 0.1) !important;
  transform: scale(0.95) !important;
}

.login-text::after {
  border: none !important;
}